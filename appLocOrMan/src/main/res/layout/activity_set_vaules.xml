<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bla"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="de.unimuenster.ifgi.locormandemo.SetValues"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:text="6. location: delay"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:text="4. location: low accuracy "
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text1" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:text="7. orientation: delay "
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/setLoAcc"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:textSize="18sp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/setDisAcc"
        app:layout_constraintRight_toLeftOf="@+id/getLoAcc"
        android:layout_marginRight="24dp" />

    <TextView
        android:id="@+id/setOrDelay"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:layout_marginRight="24dp"
        android:layout_marginTop="32dp"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/getOrDelay"
        app:layout_constraintTop_toBottomOf="@+id/setLoDelay" />

    <TextView
        android:id="@+id/setLoDelay"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/getLoDelay"
        android:layout_marginRight="24dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/setLoAcc" />

    <TextView
        android:id="@+id/setOrAcc"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintRight_toLeftOf="@+id/getOrAcc"
        android:layout_marginRight="24dp" />

    <EditText
        android:id="@+id/getLoDelay"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:numeric="integer"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/getLoAcc" />

    <EditText
        android:id="@+id/getOrDelay"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:numeric="integer"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/getLoDelay" />

    <EditText
        android:id="@+id/getOrAcc"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:numeric="integer"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="33dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/getLoAcc"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:numeric="integer"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/getDisAcc"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/getDisAcc"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:numeric="integer"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/getOrAcc"
        android:layout_marginRight="-6dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/setDisAcc"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:textSize="18sp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/setOrAcc"
        app:layout_constraintRight_toLeftOf="@+id/getDisAcc"
        android:layout_marginRight="24dp" />

    <Button
        android:id="@+id/changeBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_green_light"
        android:text="Change"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="32dp"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:text="3. location: display accuracy "
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="221dp"
        android:layout_height="40dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:text="2. orientation: low accuracy"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
